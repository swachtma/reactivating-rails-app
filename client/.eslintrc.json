{
  "extends": "standard",
  "parser": "babel-eslint",
  "plugins": [
    "react"
  ],
  "env": {
    "browser": true,
    "jest": true
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "camelcase": 0,
    "react/jsx-uses-react": 1,
    "react/jsx-uses-vars": 1,
    "no-unused-vars": ["error", { "varsIgnorePattern": "configEnzyme" }]
  }
}